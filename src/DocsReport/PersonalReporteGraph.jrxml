     tbTipoPersonal."tipo_personal" AS tbTipoPersonal_tipo_personal
FROM
     "dbo"."tbTipoPersonal" tbTipoPersonal INNER JOIN "dbo"."tbPersonal" tbPersonal ON tbTipoPersonal."idTipoPersonal" = tbPersonal."idTipoPersonal"
